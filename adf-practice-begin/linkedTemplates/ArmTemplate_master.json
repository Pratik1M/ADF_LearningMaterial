{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-practice-begin"
		},
		"Azbssource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azbssource'"
		},
		"Azdldestination_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Azdldestination'"
		},
		"ls_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_db'"
		},
		"Azdldestination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stdestinationcontainer.dfs.core.windows.net/"
		},
		"ls_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverpractice-orders.database.windows.net"
		},
		"ls_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-orders"
		},
		"ls_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqlserveradmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-practice-begin_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azbssource_connectionString": {
						"value": "[parameters('Azbssource_connectionString')]"
					},
					"Azdldestination_accountKey": {
						"value": "[parameters('Azdldestination_accountKey')]"
					},
					"ls_sql_db_password": {
						"value": "[parameters('ls_sql_db_password')]"
					},
					"Azdldestination_properties_typeProperties_url": {
						"value": "[parameters('Azdldestination_properties_typeProperties_url')]"
					},
					"ls_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_server')]"
					},
					"ls_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_database')]"
					},
					"ls_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-practice-begin_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-practice-begin_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-practice-begin_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-practice-begin_ArmTemplate_1')]"
			]
		},
		{
			"name": "adf-practice-begin_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-practice-begin_ArmTemplate_2')]"
			]
		}
	]
}